{
  "name": "trillo",
  "version": "1.0.0",
  "description": "trillo app page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Priyansh-09/Trillo.git"
  },
  "author": "Priyansh",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.0.0"
  },
  "bugs": {
    "url": "https://github.com/Priyansh-09/Trillo/issues"
  },
  "homepage": "https://github.com/Priyansh-09/Trillo#readme"
}
